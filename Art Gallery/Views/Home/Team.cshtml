
@{
    ViewBag.Title = "Team";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- start parallax scrolling section -->
<section class="wow fadeIn no-padding parallax one-fourth-screen sm-height-500px xs-height-350px" data-stellar-background-ratio="0.5" style="background-image: url('../Content/images/paintings/train.jpg')">
    <div class="container">
        <div class="row">
            <div class="col-md-12"></div>
        </div>
    </div>            
    <div class="down-section text-center"><a href="#info" class="inner-link"><i class="ti-arrow-down icon-extra-small text-white bg-deep-pink padding-15px-all xs-padding-10px-all border-radius-100"></i></a></div>
</section>
<!-- end parallax scrolling section -->
<!-- start section -->
<section id ="info" class="wow fadeIn">
    <div class="container">
        <div class="row">
            <div class="col-md-4 col-sm-12 text-left sm-margin-30px-bottom sm-padding-80px-lr xs-padding-15px-lr sm-text-center wow fadeIn">
                <h5 class="alt-font font-weight-700 text-extra-dark-gray text-uppercase width-80 md-width-100">Gallerie d'Art</h5>
                <div class="separator-line-verticle-extra-small bg-extra-dark-gray width-50 sm-width-70 sm-center-col margin-30px-bottom sm-margin-20px-bottom"></div>
                <p class="width-95 md-width-100">We are idea-driven, working with a strong focus on design and user experience. Gallerie d'Art should bring out the hidden artist within you.</p>
                <a class="btn btn-small btn-dark-gray font-weight-700 inner-link" href="#team">Meet the team</a>
            </div>
        </div>
    </div>
</section>
<!-- end section -->
<!-- start team slider section -->
<section id="team" class="wow fadeIn">
    <div class="container">
        <div class="row">
            <div class="position-relative">
                <div class="swiper-container black-move blog-slider swiper-four-slides swiper-pagination-bottom">
                    <div class="swiper-wrapper">
                        <!--  start team item -->
                        <div class="swiper-slide padding-15px-lr team-block text-left team-style-1 md-margin-40px-bottom wow fadeInUp">
                            <figure>
                                <div class="team-image xs-width-100">
                                    <img src="../Content/images/team/Ather.jpg" alt="">
                                </div>
                                <figcaption>
                                    <div class="team-member-position margin-20px-top text-center">
                                        <div class="text-small font-weight-500 text-extra-dark-gray text-uppercase">Ather Fawaz</div>
                                        <div class="text-extra-small text-uppercase text-medium-gray">17-4004</div>
                                    </div>
                                </figcaption>
                            </figure>
                        </div>
                        <!-- end team item -->
                        <!--  start team item -->
                        <div class="swiper-slide padding-15px-lr team-block text-left team-style-1 md-margin-40px-bottom wow fadeInUp">
                            <figure>
                                <div class="team-image xs-width-100">
                                    <img src="../Content/images/team/Osama.jpg" alt="">
                                </div>
                                <figcaption>
                                    <div class="team-member-position margin-20px-top text-center">
                                        <div class="text-small font-weight-500 text-extra-dark-gray text-uppercase">Osama Asif</div>
                                        <div class="text-extra-small text-uppercase text-medium-gray">17-4295</div>
                                    </div>
                                </figcaption>
                            </figure>
                        </div>
                        <!-- end team item -->
                        <!--  start team item -->
                        <div class="swiper-slide padding-15px-lr team-block text-left team-style-1 md-margin-40px-bottom wow fadeInUp">
                            <figure>
                                <div class="team-image xs-width-100">
                                    <img src="../Content/images/team/Murad.jpg" alt="">
                                </div>
                                <figcaption>
                                    <div class="team-member-position margin-20px-top text-center">
                                        <div class="text-small font-weight-500 text-extra-dark-gray text-uppercase">Hamza Murad</div>
                                        <div class="text-extra-small text-uppercase text-medium-gray">17-4125</div>
                                    </div>
                                </figcaption>
                            </figure>
                        </div>
                        <!-- end team item -->
                        <!--  start team item -->
                        <div class="swiper-slide padding-15px-lr team-block text-left team-style-1 md-margin-40px-bottom wow fadeInUp">
                            <figure>
                                <div class="team-image xs-width-100">
                                    <img src="../Content/images/team/Mujtaba.jpg" alt="">
                                </div>
                                <figcaption>
                                    <div class="team-member-position margin-20px-top text-center">
                                        <div class="text-small font-weight-500 text-extra-dark-gray text-uppercase">Malik Muhammad Mujtaba</div>
                                        <div class="text-extra-small text-uppercase text-medium-gray">17-4144</div>
                                    </div>
                                </figcaption>
                            </figure>
                        </div>
                        <!-- end team item -->
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- end team slider section -->
